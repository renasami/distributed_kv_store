{
  "name": "distributed_kv_store",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:cluster": "concurrently \"npm run node:1\" \"npm run node:2\" \"npm run node:3\"",
    "node:1": "NODE_ID=node1 PORT=3001 ts-node-dev src/index.ts",
    "node:2": "NODE_ID=node2 PORT=3002 ts-node-dev src/index.ts",
    "node:3": "NODE_ID=node3 PORT=3003 ts-node-dev src/index.ts",
    "build": "rimraf dist && tsc",
    "build:prod": "rimraf dist && esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --minify",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "cli": "ts-node src/cli/index.ts",
    "clean": "rimraf dist coverage .nyc_output",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/websocket": "^11.2.0",
    "@msgpack/msgpack": "^3.1.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@types/inquirer": "^9.0.9",
    "@types/levelup": "^5.1.5",
    "@types/lodash": "^4.17.20",
    "axios": "^1.11.0",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "debug": "^4.4.1",
    "fastify": "^5.5.0",
    "inquirer": "^12.9.4",
    "leveldown": "^6.1.1",
    "levelup": "^5.1.1",
    "lodash": "^4.17.21",
    "msgpack-lite": "^0.1.26",
    "nanoid": "^5.1.5",
    "node-fetch": "^3.3.2",
    "ora": "^8.2.0",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "prom-client": "^15.1.3",
    "protobufjs": "^7.5.4",
    "rocksdb": "^5.2.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^4.1.4"
  }
}
